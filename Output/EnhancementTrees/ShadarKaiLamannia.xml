         <Effect>
            <Type>SneakAttackDamage</Type>
            <Bonus>Enhancement</Bonus>
            <Dice>
               <Number size="1">1</Number>
               <Sides size="1">6</Sides>
            </Dice>
            <Energy>Cold</Energy>
         </Effect>
