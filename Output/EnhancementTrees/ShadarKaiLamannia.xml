              <Type>Incorpreality</Type>
              <Bonus>Enhancement</Bonus>
              <Amount>25</Amount>
              <Stance>Ghostly Essence</Stance>
              <ApplyAsItemEffect/>
          </Effect>
      </EnhancementTreeItem>
